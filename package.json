{
  "name": "open-prost",
  "description": "The future is liquid.",
  "version": "0.1.0",
  "homepage": "https://github.com/dasantonym/open-prost",
  "keywords": [
    "liquid",
    "future",
    "awesome",
    "prostate",
    "artproject"
  ],
  "license": "WTFPL",
  "repository": {
    "type": "git",
    "url": "git://github.com/dasantonym/open-prost.git"
  },
  "author": "Anton K. / GÃ¤ngeviertel e.V. <info@das-gaengeviertel.info>",
  "contributors": [],
  "bugs": {
    "url": "https://github.com/dasantonym/open-prost/issues"
  },
  "engines": {
    "node": ">= 4.5"
  },
  "main": "ui/index.html",
  "node-main": "server/index.js",
  "js-flags": "--harmony",
  "window": {
    "title": "OpenProst",
    "icon": "assets/openprost-icon-256.png",
    "toolbar": true,
    "frame": true,
    "width": 1280,
    "height": 720,
    "min-width": 1024,
    "min-height": 600
  },
  "scripts": {
    "start": "node build/server/",
    "release": "npm run clean && npm run babel && npm run build && npm run test && npm run build-desktop",
    "build": "npm run babel-nw && npm run webpack && npm run pug && npm run less",
    "test": "npm run jshint && npm run mocha",
    "jshint": "jshint src/. test/. --config",
    "babel": "babel src/server/ --out-dir build/server/",
    "babel-nw": "babel src/ui/ --out-dir build/desktop/",
    "webpack": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "webpack-dev": "cross-env NODE_ENV=dev webpack --progress --hide-modules",
    "webpack-watch": "cross-env NODE_ENV=dev webpack --watch --progress --hide-modules",
    "pug": "pug src/ui/*.pug --out build/desktop/",
    "pug-watch": "pug -w src/ui/index.pug --out build/desktop/",
    "less": "lessc --clean-css='--s1 --advanced' ./src/ui/index.less ./build/desktop/openprost-ui.css",
    "clean": "rm -rf ./build && rm -rf ./dist",
    "copy": "cp ./package.json build/desktop/ && cp -r ./build/server ./build/desktop/ && cp -r ./assets build/desktop/ && cp -r ./config ./build/desktop/",
    "debug-desktop": "npm run copy && cross-env NODE_ENV=dev nwb nwbuild -v 0.20.1-sdk -r -- --remote-debugging-port=9222 ./build/desktop/",
    "build-desktop": "npm run copy && cross-env NODE_ENV=production nwb nwbuild --win-ico assets/openprost.ico --mac-icns assets/openprost.icns -o ./dist --side-by-side -p osx64 -v 0.20.1 ./build/desktop/",
    "mocha": "mocha test/ --recursive --timeout 4000"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "babel-runtime": "^6.23.0",
    "bluebird": "^3.4.7",
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "debug": "^2.6.1",
    "element-ui": "^1.1.6",
    "feathers": "^2.1.0",
    "feathers-authentication": "^0.7.12",
    "feathers-configuration": "^0.4.1",
    "feathers-errors": "^2.5.0",
    "feathers-hooks": "^1.7.1",
    "feathers-nedb": "^2.6.1",
    "feathers-rest": "^1.6.0",
    "moment": "^2.17.1",
    "nedb": "^1.8.0",
    "passport": "^0.3.2",
    "serve-favicon": "^2.4.0",
    "superagent": "^3.4.4",
    "vue": "^2.2.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.23.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "file-loader": "^0.11.0",
    "jshint": "^2.9.4",
    "less": "^2.7.2",
    "less-plugin-clean-css": "^1.5.1",
    "mocha": "^3.2.0",
    "nwjs-builder": "^1.14.0",
    "pug-cli": "^1.0.0-alpha6",
    "request": "^2.79.0",
    "style-loader": "^0.14.0",
    "webpack": "^2.2.1",
    "webpack-node-externals": "^1.5.4"
  },
  "stability": "unstable"
}
