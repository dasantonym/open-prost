{
  "name": "open-prost",
  "description": "The future is liquid.",
  "version": "0.1.0",
  "homepage": "",
  "main": "src/",
  "keywords": [
    "feathers"
  ],
  "license": "MIT",
  "repository": {},
  "author": {},
  "contributors": [],
  "bugs": {},
  "engines": {
    "node": ">= 4.5"
  },
  "scripts": {
    "start": "node dist/server/",
    "release": "npm run clean && npm run babel && npm run build && npm run test",
    "build": "npm run webpack && npm run pug && npm run less",
    "test": "npm run jshint && npm run mocha",
    "jshint": "jshint src/. test/. --config",
    "babel": "babel src/server/ --out-dir dist/server/",
    "webpack": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "webpack-dev": "cross-env NODE_ENV=dev webpack --progress --hide-modules",
    "webpack-watch": "cross-env NODE_ENV=dev webpack --watch --progress --hide-modules",
    "pug": "pug src/ui/index.pug --out dist/ui/",
    "less": "lessc --clean-css='--s1 --advanced' ./src/ui/index.less ./dist/ui/openprost-ui.css",
    "clean": "rm -r ./dist",
    "mocha": "mocha test/ --recursive"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "babel-runtime": "^6.23.0",
    "bluebird": "^3.4.7",
    "body-parser": "^1.16.1",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "debug": "^2.6.1",
    "element-ui": "^1.1.6",
    "feathers": "^2.0.3",
    "feathers-authentication": "^0.7.12",
    "feathers-configuration": "^0.3.3",
    "feathers-errors": "^2.5.0",
    "feathers-hooks": "^1.7.1",
    "feathers-nedb": "^2.6.1",
    "feathers-rest": "^1.6.0",
    "moment": "^2.17.1",
    "nedb": "^1.8.0",
    "passport": "^0.3.2",
    "serve-favicon": "^2.4.0",
    "superagent": "^3.4.4",
    "vue": "^2.1.10",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.23.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "file-loader": "^0.10.0",
    "jshint": "^2.9.4",
    "less": "^2.7.2",
    "less-plugin-clean-css": "^1.5.1",
    "mocha": "^3.2.0",
    "nwjs-builder": "^1.14.0",
    "pug-cli": "^1.0.0-alpha6",
    "request": "^2.79.0",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-node-externals": "^1.5.4"
  },
  "stability": "unstable"
}
